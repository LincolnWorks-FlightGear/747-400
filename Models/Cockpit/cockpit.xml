<!-- SPEEDBRAKE LEVER -->

<animation>
	<type>knob</type>
	<object-name>speedbrake</object-name>
	<drag-direction>vertical</drag-direction>
	<drag-scale-px>25</drag-scale-px>
	<property>/autopilot/autospoilers/step</property>
	<action>
		<binding>
			<command>property-adjust</command>
			<property>/autopilot/autospoilers/step</property>
			<factor>-1</factor>
			<min>0</min>
			<max>3</max>
		</binding>
	</action>
	<interpolation>
		<entry>
			<ind>0</ind>
			<dep>-3</dep>
		</entry>
		<entry>
			<ind>1</ind>
			<dep>0</dep>
		</entry>
		<entry>
			<ind>2</ind>
			<dep>34</dep>
		</entry>
		<entry>
			<ind>3</ind>
			<dep>68</dep>
		</entry>
	</interpolation>
	<center>
		<x-m>1.143</x-m><y-m>0</y-m><z-m>0.362</z-m>
	</center>
	<axis>
		<y>1</y>
	</axis>
</animation>

