<!-- FLAPS LEVER -->

<animation>
	<type>knob</type>
	<object-name>flap_handle</object-name>
	<drag-direction>vertical</drag-direction>
	<property>/controls/flight/flaps</property>
	<increase>
		<binding>
			<command>nasal</command>
			<script>controls.flapsDown(-1);</script>
		</binding>
	</increase>
	<decrease>
		<binding>
			<command>nasal</command>
			<script>controls.flapsDown(1);</script>
		</binding>
	</decrease>
	<hovered>
		<binding>
			<command>set-tooltip</command>
			<label>Flaps: %2d</label>
			<tooltip-id>flaps</tooltip-id>
			<property>/fdm/jsbsim/fcs/flap-cmd-deg</property>
		</binding>
	</hovered>
	<interpolation>
		<entry>
			<ind>0</ind>
			<dep>0.00</dep>
		</entry>
		<entry>
			<ind>0.001</ind>
			<dep>10.83</dep>
		</entry>
		<entry>
			<ind>0.167</ind>
			<dep>21.67</dep>
		</entry>
		<entry>
			<ind>0.333</ind>
